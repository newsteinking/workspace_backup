def sum_list(slist):
    result=1
    for i in slist:
        result=result*i
    return result
a=[3,4,6,9] 
print(sum_list(a))

def Nsum(n):
    number=0
    for num in range(1,n+1):
        number=number+num
    return number
print(Nsum(10))

def nsum(numlist):
    result=0
    #for Num in numlist:  len(a)
    for num in range(1,len(numlist)):
        #print(num)
        result=result+abs((numlist[num]-numlist[num-1]))
    return result
b=[3,4,7,4]
print(nsum(b))