def sum_list(slist):
    result=1
    for i in slist:
        result=result*i
    return result
a=[3,4,6,9] 
print(sum_list(a))

def Nsum(n):
    number=0
    for num in range(1,n+1):
        number=number+num
    return number
print(Nsum(10))

