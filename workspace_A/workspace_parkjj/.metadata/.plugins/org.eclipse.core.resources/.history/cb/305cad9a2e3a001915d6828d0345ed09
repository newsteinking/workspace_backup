from mcpi.minecraft import Minecraft
import time
import random
logan=Minecraft.create()

def brockenblock():
    brockenblocks=[48,67,4,4,4,4]
    block=random.choice(brockenblocks)
    return block
x,y,z=logan.player.getPos()
brokenwalls=[]
height=5
witdh=10
for i in range(height):
    brokenwalls.append([])
    for j in range(witdh):
        block=brockenBlock()
        brokenwalls[i].append(block)
        
print(brokenwalls)
        
    