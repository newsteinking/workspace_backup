from mcpi.minecraft import Minecraft
mc=Minecraft.create()
import time
import random


blocks=[56,67,42,36,98]

#block=ramdom.choice(blocks)

x,y,z=mc.player.getPos()

for i in range(6):
    block=random.choice(blocks)
    mc.setBlock(x+5,y+i,z+2,block)
   
